<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="lollipop" android:color="@color/light_grey">
    <item >
        <shape>
            <size android:width="@dimen/circle_radius" android:height="@dimen/circle_radius" />
            <corners android:radius="@dimen/circle_radius" />
            <stroke android:color="@color/cardview_dark_background" android:width="1dp" />
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <size android:width="@dimen/circle_radius" android:height="@dimen/circle_radius" />
            <corners android:radius="@dimen/circle_radius" />
            <solid android:color="@color/light_grey" />
        </shape>
    </item>
</ripple>